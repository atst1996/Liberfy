<Application x:Class="Liberfy.UIPreview.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Liberfy.UIPreview"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Liberfy;component/Resources/Icons.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Liberfy;component/Themes/Generic.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Liberfy;component/Resources/ControlStyles.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Liberfy;component/Resources/DataTemplates.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
